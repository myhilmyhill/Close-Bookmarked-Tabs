{
    "browser_action": {
        "browser_style": true,
        "default_title": "Close Bookmarked Tabs...",
        "default_popup": "index.html"
    },
    "manifest_version": 2,
    "name": "Close Bookmarked Tabs",
    "version": "0.0b",
    "permissions": [
        "tabs",
        "bookmarks"
    ],
    "browser_specific_settings": {
        "gecko": {
            "update_url": "https://raw.githubusercontent.com/myhilmyhill/Close-Bookmarked-Tabs/master/updates.json"
        }
    }
}